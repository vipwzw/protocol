{
    "name": "@0x/contract-addresses",
    "version": "8.12.0",
    "engines": {
        "node": ">=6.12"
    },
    "description": "Used to get known addresses of deployed 0x contracts",
    "main": "lib/src/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "yarn tsc -b",
        "build:ts": "tsc -b",
        "build:ci": "yarn build",
        "test": "yarn build && yarn test:hardhat",
        "test:hardhat": "yarn build && npx hardhat test",
        "test:ci": "yarn build && yarn test",
        "test:coverage": "nyc --reporter=text --reporter=html --reporter=lcov yarn test",
        "clean": "shx rm -rf lib generated_docs coverage .nyc_output *.log",
        "clean:node-modules": "shx rm -rf node_modules",
        "clean:all": "yarn clean && yarn clean:node-modules",
        "publish:private": "yarn build && echo 'gitpkg publish disabled'",
        "lint": "eslint src test --ext .ts,.js",
        "lint:fix": "eslint src test --ext .ts,.js --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/protocol.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/0xProject/protocol/issues"
    },
    "homepage": "https://github.com/0xProject/protocol/tree/main/packages/contract-addresses",
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "chai": "^4.5.0",
        "ethereumjs-util": "^7.1.5",
        "mocha": "^10.2.0",
        "shx": "^0.4.0",
        "ts-node": "^10.9.0",
        "typescript": "^5.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "4f91bfd907996b2f4dd383778b50c479c2602b56"
}
