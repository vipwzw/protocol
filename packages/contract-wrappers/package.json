{
    "name": "@0x/contract-wrappers",
    "version": "13.23.7",
    "engines": {
        "node": ">=6.12"
    },
    "description": "Wrappers for 0x smart contract wrappers generated using TypeChain",
    "keywords": [
        "0xproject",
        "ethereum",
        "tokens",
        "exchange",
        "typechain"
    ],
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "scripts": {
        "build": "yarn typechain:generate && tsc -b",
        "rebuild": "yarn typechain:clean && yarn typechain:generate && yarn typechain:prettier && yarn build",
        "build:ci": "yarn build",
        "publish:private": "yarn rebuild && echo 'gitpkg publish disabled'",
        "lint": "eslint src --ext .ts,.js",
        "fix": "eslint --fix src",
        "prettier": "prettier --write **/* --config ../../.prettierrc",
        "clean": "shx rm -rf lib generated_docs coverage .nyc_output *.log",
        "clean:node-modules": "shx rm -rf node_modules",
        "clean:all": "yarn clean && yarn clean:node-modules",
        "docs_test": "typedoc --excludePrivate --excludeExternals --target ES5 --tsconfig typedoc-tsconfig.json --out generated_docs ./src/typechain-types/*",
        "diff_docs": "git diff --exit-code ./docs",
        "s3:sync_md_docs": "aws s3 sync ./docs s3://docs-markdown/${npm_package_name}/v${npm_package_version} --profile 0xproject --region us-east-1 --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
        "docs:md": "ts-doc-gen --sourceDir='$PROJECT_FILES' --output=$MD_FILE_DIR --fileExtension=mdx --tsconfig=./typedoc-tsconfig.json",
        "docs:json": "typedoc --excludePrivate --excludeExternals --excludeProtected --ignoreCompilerErrors --target ES5 --tsconfig typedoc-tsconfig.json --json $JSON_FILE_PATH $PROJECT_FILES",
        "typechain:prettier": "if [ -d \"src/typechain-types\" ] && [ \"$(ls -A src/typechain-types)\" ]; then prettier --write src/typechain-types/* --config ../../.prettierrc; fi",
        "typechain:clean": "rm -rf src/typechain-types",
        "typechain:generate": "npx typechain --target ethers-v6 --out-dir src/typechain-types '../contract-artifacts/artifacts/*.json'",
        "test": "hardhat test",
        "test:coverage": "nyc --reporter=text --reporter=html --reporter=lcov yarn test && echo '\nCoverage report generated in coverage/ directory'"
    },
    "config": {
        "abis": "../contract-artifacts/artifacts/@(DevUtils|ERC20Token|ERC721Token|Exchange|Forwarder|IAssetData|LibTransactionDecoder|WETH9|Coordinator|Staking|StakingProxy|GodsUnchainedValidator|Broker|ILiquidityProvider|ILiquidityProviderRegistry|MaximumGasPrice|ITransformERC20|IZeroEx).json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/protocol.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/0xProject/protocol/issues"
    },
    "homepage": "https://github.com/0xProject/protocol/tree/main/packages/contract-wrappers",
    "devDependencies": {
        "@typechain/ethers-v6": "^0.5.1",
        "@typechain/hardhat": "^9.1.0",
        "chai": "^4.5.0",
        "hardhat": "^2.26.1",
        "shx": "^0.4.0",
        "typechain": "^8.3.2",
        "typedoc": "~0.16.11",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "ethereum-types": "^3.7.1",
        "ethers": "^6.15.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "4f91bfd907996b2f4dd383778b50c479c2602b56"
}
